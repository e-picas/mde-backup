{
    "name": "atelierspierrot/extended-markdown",
    "type": "parser",
    "title": "PHP Extended Markdown",
    "description": "An extended version of PHP Markdown parser as a Composer package to use in other projects",
    "keywords": [ "markdown", "parser" ],
    "homepage": "https://github.com/atelierspierrot/extended-markdown",
    "version": "1.0.1",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "John Gruber",
            "homepage": "http://daringfireball.net/"
        },
        {
            "name": "Michel Fortin",
            "homepage": "http://michelf.com/"
        },
        {
            "name": "Pierre Cassat",
            "email": "piero.wbmstr@gmail.com",
            "homepage": "https://github.com/PieroWbmstr"
        }
    ],
    "require": { "php": ">=5.3.0" },
    "autoload": { "psr-0": { "Markdown": "src" } },
    "config": { "bin-dir": "bin" },
    "bin": ["bin/emd_console"]
}
